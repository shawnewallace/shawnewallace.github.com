---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">Vito's journal</title>
    <link type="application/atom+xml" href="http://blog.shawnewallace.com/feed/" rel="self"/>
    <link type="text" href="http://blog.shawnewallace.com" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://blog.shawnewallace.com</id>
    <author>
        <name>Shawn Wallace</name>
    </author>
    <rights>Copyright (c) 2010-2012 Shawn Wallace</rights>
    {% for post in site.posts limit:20 %}
    <entry>
        <title></title>
        <link href="http://blog.shawnewallace.com{{ post.url }}/"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://blog.shawnewallace.com{{ post.url }}/</id>
        <summary type="html">{{ post.excerpt | xml_escape }}</summary>
    </entry>
    {% endfor %}
</feed>