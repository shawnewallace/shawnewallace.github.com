---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Shawn Wallace Blog</title>
  <link href="http://www.shawnewallace.com/atom.xml" rel="self"/>
  <link href="http://www.shawnewallace.com/"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://shawnewallace.com/</id>
  <author>
    <name>Christopher Sexton</name>
    <email></email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://blog.shawnewallace.com"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://codeography.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>